<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_before_today</fullName>
    <active>true</active>
    <errorConditionFormula>adoption_date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>adoption_date__c</errorDisplayField>
    <errorMessage>You cannot create an adoption with a date before today</errorMessage>
</ValidationRule>
